using System;
using System.Linq;

namespace _1._Action_Point
{
    class Program
    {
        static void Main(string[] args)
        {
            string[] names = Console.ReadLine().Split().ToArray();
            Action<string[]> print = name => Console.WriteLine(string.Join(Environment.NewLine, name));
            print(names);
         

        }
    }
}
